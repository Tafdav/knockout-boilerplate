{
  "extends": "tslint:recommended",
  "rules": {
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": true,
    "member-access": [
      false
    ],
    "quotemark": [
      false
    ],
    "max-line-length": [
      false
    ],
    "trailing-comma": [
      true,
      {
        "multiline": {
          "functions": "never"
        }
      }
    ]
  }
}